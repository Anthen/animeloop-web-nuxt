{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [ "> 1%"]
      },
      "exclude": ["transform-es2015-arrow-functions"]
      }]
    ],
    "plugins": [
      "syntax-dynamic-import",
      "transform-async-to-generator",
      "transform-regenerator",
      "transform-runtime",
      "transform-object-rest-spread"
    ]
  }
